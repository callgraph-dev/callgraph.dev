{
  "name": "callgraph.dev-frontend",
  "version": "1.0.0",
  "description": "frontend for callgraph.dev",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:html && npm run build:css && npm run build:ts",
    "build:html:this:is:wrong": "mkdir -p ./dist && cp ./static/index.html ./dist/index.html",
    "watch": "docker compose -f ../development/docker-compose.development.yml up",
    "build:css": "tailwindcss -i ./src/tailwind.css -o ../backend/media/react-vscode-bundle.css",
    "build:ts": "esbuild index.tsx --bundle --minify --platform=browser --target=es2020 --outfile=../backend/media/react-vscode-bundle.js --define:process.env.SERVER_URL=\\\"http://localhost:3000\\\" --define:process.env.FEATURE_FILEEXPLORER=\\\"false\\\" --define:process.env.FEATURE_COMMAND_PALETTE=\\\"false\\\" --define:process.env.FEATURE_VSCODE_EXTENSION=\\\"true\\\"",
    "watch:css": "tailwindcss --watch -i ./src/tailwind.css -o ../backend/media/react-vscode-bundle.css",
    "watch:ts": "esbuild index.tsx --watch --bundle --minify --platform=browser --target=es2020 --outfile=../backend/media/react-vscode-bundle.js --define:process.env.SERVER_URL=\\\"http://localhost:3000\\\" --define:process.env.FEATURE_FILEEXPLORER=\\\"false\\\" --define:process.env.FEATURE_COMMAND_PALETTE=\\\"false\\\" --define:process.env.FEATURE_VSCODE_EXTENSION=\\\"true\\\""
  },
  "author": "Vivek Dasari",
  "license": "MIT",
  "dependencies": {
    "@floating-ui/dom": "^1.6.3",
    "@floating-ui/react-dom": "^2.0.8",
    "@headlessui/react": "^2.0.0-alpha.4",
    "@reduxjs/toolkit": "^2.1.0",
    "clsx": "^2.1.0",
    "cytoscape": "3.28.1",
    "cytoscape-dagre": "2.5.0",
    "cytoscape-popper": "github:Atrue/cytoscape.js-popper#v3",
    "dagre": "0.8.5",
    "highlight.js": "^11.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@types/cytoscape": "^3.19.16",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "esbuild": "^0.20.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1"
  }
}
